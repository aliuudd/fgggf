[app]
title = TelegramBotApp
package.name = smsdomainm
package.domain = org.telegram.bot
source.dir = .
source.include_exts = py
version = 1.0
requirements = python3, pyTelegramBotAPI
orientation = portrait
fullscreen = 0
android.permissions = INTERNET,WRITE_EXTERNAL_STORAGE,READ_EXTERNAL_STORAGE

[buildozer]
log_level = 2
warn_on_root = 1
